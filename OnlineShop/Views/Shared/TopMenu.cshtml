@model List<Model.EF.MenuBar>
<div class="call">
    <p><span>Need help?</span> call us <span class="number">0917-246-268</span></span></p>
</div>
<div class="account_desc">
    @{
        var session = (UserLogin)Session[OnlineShop.Common.CommonConstants.USER_SESSION];
    }
    @if (session == null)
    {
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
            }
        </ul>
    }
    else
    {
        <ul>
            <li>Xin chào : @session.UserName</li>
            <li>@Html.ActionLink("Đăng xuất","Logout","User")</li>
        </ul>
    }
</div>
<div class="clear"></div>
